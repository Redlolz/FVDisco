        <script language="javascript" type="text/javascript">
		
		var theWarnings = 0;
		theWarnings =0;
		//0
		var theWarningsDesc = "";
		
		if(theWarnings > 10){
			theWarningDesc = "BINNED";
		}
			
		var theImage = "0.png";
		
		switch(theWarnings)
		{
			case 0:
			  theImage = "0.png";
			  theWarningDesc = "No Warnings";
			  break;
			case 1:
			  theImage = "1.png";
			  theWarningDesc = "Slap on the Wrist";
			  break;
			case 2:
			  theImage = "2.png";
			  theWarningDesc = "Naughty";
			  break;
			case 3:
			  theImage = "3.png";
			  theWarningDesc = "Thin Ice";
			  break;
			case 4:
			  theImage = "4.png";
			  theWarningDesc = "One Foot in the Bin";
			  break;
			case 5:
			  theImage = "5.png";
			  theWarningDesc = "BIN!";
			  break;
			default:
			  theImage = "5.png";
			  break;
		}
		
		
		var theWarningPercent = theWarnings/5;
		theWarningPercent = theWarningPercent*100;
		theWarningPercent = Math.round(theWarningPercent);
		
		var displayit = theWarningPercent + "% <br />" + "<img src='/images/" + theImage + "' alt='" + theWarningDesc + "' /> <br />" +  theWarningDesc + "</dd>";	
		
		</script>
        
       <dd><strong>Warning Level: </strong><script type="text/javascript">document.write(displayit);</script></dd>

		

