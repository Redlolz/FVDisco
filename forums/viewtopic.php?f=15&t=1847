			  theImage = "0.png";
			  theWarningDesc = "No Warnings";
			  break;
			case 1:
			  theImage = "1.png";
			  theWarningDesc = "Slap on the Wrist";
			  break;
			case 2:
			  theImage = "2.png";
			  theWarningDesc = "Naughty";
			  break;
